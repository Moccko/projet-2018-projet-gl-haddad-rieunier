<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Domain" namespace="Domain">

  <class name="Coureur" table="coureur">
    <id name="Id" column="id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Annee" column="annee" not-null="true"/>
    <property name="Courriel" column="courriel" not-null="true"/>
    <property name="Nom" column="nom" not-null="false"/>
    <property name="Prenom" column="prenom" not-null="false"/>
    <property name="Sexe" column="sexe" not-null="false"/>
    <property name="NoLicenceFFA" column="noLicenceFFA" not-null="false"/>
  </class>

</hibernate-mapping>